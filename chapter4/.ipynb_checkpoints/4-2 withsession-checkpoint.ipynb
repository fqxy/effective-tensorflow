   "source": [
    "import tensorflow as tf\n",
    "a = tf.constant(3)                     #定义常量3\n",
    "b = tf.constant(4)                     #定义常量4\n",
    "with tf.Session() as sess:             #建立session\n",
    "    print (\"相加: %i\" % sess.run(a+b))\n",
    "    print( \"相乘: %i\" % sess.run(a*b))\n",
    "    # 不用写sess.close()"
   ]
  }
 ],
